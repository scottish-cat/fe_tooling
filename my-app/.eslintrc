{
    "extends": [
      "react-app",
      "airbnb",
      "plugin:jsx-a11y/recommended",
      "prettier",
      "prettier/react"
    ],
    "plugins": [
      "jsx-a11y",
      "prettier"
    ],
    "rules": {
      "semi": 0,
      "no-use-before-define": ["error", { "functions": false, "classes": false }],
      "no-console": "off",
      "no-param-reassign": "off",
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "prettier/prettier": [
        "error", {
          "semi": true,
          "singleQuote": true,
          "arrowParens": "avoid"
        }
      ]
    }
  }